{
  "name": "ol-keycloakify",
  "version": "0.0.30",
  "description": "Keycloakify theme for Open Learning",
  "repository": {
    "type": "git",
    "url": "git://github.com/mitodl/ol-keycloakify.git"
  },
  "type": "module",
  "scripts": {
    "postinstall": "keycloakify sync-extensions",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-keycloak-theme": "npm run build && keycloakify build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "yarn build-storybook && serve -s storybook-static",
    "start": "keycloakify start-keycloak",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/emails/ src/login/",
    "lint:fix": "yarn lint --fix",
    "format": "prettier \"src/emails/**/*.{ts,tsx}\" \"src/login/**/*.{ts,tsx}\" --check",
    "format:fix": "prettier \"src/emails/**/*.{ts,tsx}\" \"src/login/**/*.{ts,tsx}\" --write"
  },
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@keycloak/keycloak-admin-client": "26.5.2",
    "@keycloakify/keycloak-account-ui": "~260502.0.0",
    "@keycloakify/keycloak-ui-shared": "~260502.0.0",
    "@mitodl/smoot-design": "^6.10.0",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "^7.1.1",
    "@patternfly/patternfly": "^5.4.2",
    "@patternfly/react-core": "^5.4.14",
    "@patternfly/react-icons": "^5.4.2",
    "@patternfly/react-styles": "^5.4.1",
    "@patternfly/react-table": "^5.4.16",
    "@posthog/react": "^1.5.2",
    "@remixicon/react": "^4.6.0",
    "@zootools/email-spell-checker": "^1.12.0",
    "i18next": "^25.2.1",
    "i18next-fetch-backend": "^6.0.0",
    "jsx-email": "^2.7.2",
    "keycloak-js": "^26.2.0",
    "keycloakify": "^11.8.27",
    "lodash-es": "^4.17.21",
    "posthog-js": "^1.319.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.59.0",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4",
    "@eslint/js": "^9.15.0",
    "@storybook/addon-a11y": "^10.0.0",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-onboarding": "^10.0.0",
    "@storybook/addon-vitest": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-vite": "^10.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^10.0.0",
    "globals": "^16.0.0",
    "keycloakify-emails": "^3.1.0",
    "playwright": "^1.52.0",
    "prettier": "^3.6.2",
    "storybook": "^10.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "packageManager": "yarn@4.11.0"
}
